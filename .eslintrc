{
  "root": true,
  "parser": "babel-eslint",
  "plugins": [
    "babel",
    "jsx-a11y",
    "import",
    "react"
  ],
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  },
  "env": {
    "es6": true
  },
  "extends": "airbnb",
  "rules": {
    "linebreak-style": 0,
    "semi": [
      2,
      "never"
    ],
    "react/jsx-filename-extension": 0
  }
}
